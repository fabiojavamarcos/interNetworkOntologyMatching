    <!-- http://purl.org/ontology/mo/ismn -->

    <owl:DatatypeProperty rdf:about="ismn">
        <rdfs:label>ismn</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI"
            >http://purl.org/ontology/mo/</rdfs:isDefinedBy>
