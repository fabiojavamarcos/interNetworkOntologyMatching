        <vs:term_status>unstable</vs:term_status>
        <level>1</level>
        <rdfs:comment
            >Method used to convert analog electronic signals into digital format such as &quot;MP3 CBR @ 128kbps&quot;, &quot;OGG @ 160kbps&quot;, &quot;FLAC&quot;, etc.</rdfs:comment>
        <rdfs:domain rdf:resource="AudioFile"/>
